import jxl.write.*
import groovy.json.JsonSlurper
import com.eviware.soapui.support.GroovyUtils

	/**
	   * File Path is hardcoded here, but a relative path can be provided if it's stored into a repository
	 */

File testDataFile = new File("/Users/dilip.rao@ibm.com/SOAPUI-Tests/MyRCAssignment/TestData/MyPetsCreationTestData.xls")
Workbook workbook = Workbook.getWorkbook(testDataFile)
	/**
	   * Assumption here is that the test data file content is in the first sheet
	 */
Sheet sheet = workbook.getSheet(0)
	/**
	   * Assumption here is that the test data has a heading and reducing the number of rows acordingly
	 */
int noOfRows = sheet.getRows()-1
def petID,petName,vaccinationStatus, reponse
def jsonSlurper = new JsonSlurper()
	/**
	   * The content is read acordingly based on the column and row index.
	 */
for(int rowIndex in 1..noOfRows){
		petID = sheet.getCell(0,rowIndex).getContents()
		petName = sheet.getCell(1,rowIndex).getContents()
		vaccinationStatus = sheet.getCell(2,rowIndex).getContents() 		
		testRunner.testCase.setPropertyValue('petID',petID)
		testRunner.testCase.setPropertyValue('petName',petName)
		testRunner.testCase.setPropertyValue('vaccinationStatus',vaccinationStatus)
		testRunner.testCase.getTestStepByName('postPetDetails').run(testRunner,context)
		responseFromPostPetDetails = context.expand('${postPetDetails#Response}')
		def response = jsonSlurper.parseText(responseFromPostPetDetails)
	/**
	   * Reading the set stattus value for assertin from the response
	 */
		def actualStatusValue = response.status		
		try {
    			assert vaccinationStatus == actualStatusValue :"Test run failed for row with petName $petName" 
    			log.info "Test run successfully for row with petName $petName"   			
			} catch (AssertionError e) {
	 /**
	   * Warning to print when the test fail along with any exception message
	 */			
			log.error "FAILED: " + e.getMessage()
	}

	}
	/**
	   * Closing the workbook, so it's not locked for editing for another user/process
	 */
workbook.close()
</script></con:config></con:testStep><con:properties><con:property><con:name>petID</con:name><con:value>104</con:value></con:property><con:property><con:name>petName</con:name><con:value>King</con:value></con:property><con:property><con:name>vaccinationStatus</con:name><con:value>Booked</con:value></con:property></con:properties></con:testCase><con:properties/></con:testSuite><con:properties/><con:wssContainer/><con:oAuth2ProfileContainer/><con:oAuth1ProfileContainer/><con:sensitiveInformation/></con:soapui-project>